<%= render "shared/errors" %>

<h1>Create an Artist Account</h1>

<%= form_for @artist do |f| %>
  <%= f.label :first_name  %>
  <%= f.text_field :first_name  %>

  <%= f.label :last_name  %>
  <%= f.text_field :last_name  %>

  <%= f.label :email  %>
  <%= f.text_field :email  %>

  <%= f.label :password  %>
  <%= f.text_field :password  %>

  <%= f.submit %>
<% end %>